local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("DudleyShoolLocks's Script Hub", "Ocean")

-- Sections And Tabs
local HelpfulScriptsTab = Window:NewTab("Helpful Scripts")
local HelpfulScriptSection = HelpfulScriptsTab:NewSection("Scripts")
local Mining Simulator 2 = Window:NewTab("TabName")

-- Buttons
HelpfulScriptSection:NewButton("InifiniteYeild", "Excutes InifiniteYeild Script", function()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'))()
end)

HelpfulScriptSection:NewButton("Anti Afk", "Don't Get Kicked For Going Afk", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/KazeOnTop/Rice-Anti-Afk/main/Wind", true))()
end)

HelpfulScriptSection:NewButton("Dark Dex", "Open The Dark Dex Explorer", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/Babyhamsta/RBLX_Scripts/main/Universal/BypassedDarkDexV3.lua", true))()
end)

HelpfulScriptSection:NewButton("Orca Hub", "Excute Orca Hub Script", function()
    loadstring(game:HttpGetAsync("https://raw.githubusercontent.com/richie0866/orca/master/public/latest.lua"))()

end)

-- Sliders
HelpfulScriptSection:NewSlider("Walk Speed", "Increase Or Decrease Walk Speed", 500, 0, function(s) -- 500 (MaxValue) | 0 (MinValue)
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = s
end)
